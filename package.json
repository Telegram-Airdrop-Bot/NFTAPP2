{
  "name": "nft-verification-portal",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@phantom/react-sdk": "^0.3.7",
    "@solana/wallet-adapter-react": "^0.15.35",
    "@solana/wallet-adapter-react-ui": "^0.9.34",
    "@solana/wallet-adapter-wallets": "^0.19.23",
    "@solana/web3.js": "^1.98.2",
    "@solflare-wallet/sdk": "^1.4.2",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "bs58": "^6.0.0",
    "cross-env": "^10.0.0",
    "react": "^18.2.0",
    "react-app-rewired": "^2.2.1",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "tweetnacl": "^1.0.3",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "cross-env GENERATE_SOURCEMAP=false react-app-rewired start",
    "dev": "cross-env GENERATE_SOURCEMAP=false react-app-rewired start",
    "build": "cross-env GENERATE_SOURCEMAP=false react-app-rewired build",
    "build:production": "cross-env GENERATE_SOURCEMAP=false NODE_ENV=production react-app-rewired build",
    "build:render": "cross-env GENERATE_SOURCEMAP=false NODE_ENV=production react-app-rewired build",
    "serve:build": "npx serve -s build",
    "test": "react-app-rewired test",
    "test:coverage": "react-app-rewired test --coverage --watchAll=false",
    "test:watch": "react-app-rewired test --watch",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "clean": "rm -rf node_modules package-lock.json && npm install --legacy-peer-deps",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.0"
  }
}





